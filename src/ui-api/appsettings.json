{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.Extensions": "Warning",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ApiInfo": {
        "ApplicationName": "Lxi-UiApi",
        "Version": 1,
        "IsPublic": false
    },
    "LaunchDarkly": {
        "Enabled": true,
        "SdkKey": "sdk-17bd183e-5a56-4556-a3a1-35e40c1e38a6"
    },
    "AzureDevOps": {
        "Organization": "your-organization",
        "Pat": "your-pat-token"
    },
    "OktaOptions": {
        "Issuer": "https://your-okta-domain.okta.com/oauth2/default",
        "ClientId": "your-client-id",
        "ClientSecret": "your-client-secret",
        "TokenUri": "https://your-okta-domain.okta.com/oauth2/default/v1/token",
        "RedirectUri": "http://localhost:8080/authorization-code/callback"
    },
    "Cors": {
        "AllowedOrigins": ["http://localhost:3000", "http://localhost:8080"]
    },
    "OpenTelemetry": {
        "Enabled": true,
        "ExporterOptions": {
            "UseTracingExporter": "otlp",
            "UseMetricsExporter": "otlp",
            "UseLoggingExporter": "otlp, nlog",
            "TelemetryCollectorUrl": "http://otel-collector:4317",
            "EnrichNLogsWithTags": true,
            "OpenTelemetrySampleRatio": 1.0,
            "MetricPointerSizeLimit": 200
        },
        "InstrumentationOptions": {
            "IsAspNetCoreInstrumentationEnabled": true,
            "IsGrpcClientInstrumentationEnabled": true,
            "IsHttpClientInstrumentationEnabled": true,
            "IsSqlClientInstrumentationEnabled": false,
            "Tags": {
                "IsActionTagEnabled": false,
                "IsGamingServerIdTagEnabled": false,
                "IsGamingServerNameTagEnabled": true,
                "IsOperatorIdTagEnabled": true,
                "IsOperatorNameTagEnabled": true,
                "IsProductGroupIdTagEnabled": false,
                "IsProductIdTagEnabled": true,
                "IsProductNameTagEnabled": true
            },
            "DataProtection": {
                "QueryParameters": []
            }
        },
        "RoleName": "lxi-ui-api",
        "DeploymentEnvironment": "docker-desktop"
    },
    "ActorOptions": {
        "ActorIdleTime": 15,
        "ActorScanInterval": 30,
        "DrainOngoingCallTimeout": 60,
        "DrainRebalancedActors": true
    },
    "ApiOptions": {
        "HostUrl": "http://host.docker.internal:5000",
        "GatewayApiBaseUrl": "http://localhost:7001"
    }
}